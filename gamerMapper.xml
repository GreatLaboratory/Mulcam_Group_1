<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gamer">
	<insert id="insert">
		insert into gamer values(#{id}, #{score})
	</insert>
<!-- 	<select id="select_id" resultType="String">select id from names</select> -->
	<select id="select" resultType="com.mulcam.typing_game.model.GamerVO">
	    select id, score from names order by score desc
	</select>

</mapper>